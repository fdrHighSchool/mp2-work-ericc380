class divideby3
{
   
    static boolean check(String str)
    {
     
        int n = str.length();
        int digitSum = 0;
        for (int i=0; i<n; i++)
            digitSum += (str.charAt(i)-'0');
      
       
        return (digitSum % 3 == 0);
    }
 
    
    public static void main (String[] args)
    {
        String str = "1332";
        if(check(str))
            System.out.println("Yes");
        else
            System.out.println("No");
    }
}
